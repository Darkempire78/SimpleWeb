{
    "resultPreviewLimit": 5,
    "resultLimit": 20,

    "defaultBrowser": "google",
    "browserPrefixes": {
        "-g": "google",
        "-d": "duckduckgo"
    },
    "browserSearch": {
        "google": "https://www.google.com/search?q=",
        "duckduckgo": "https://html.duckduckgo.com/html?q="
    },

    "spaceBetweenResults": true,

    "clear": true,
    
    "removeYoutubeResults": true,
    "removeGoogleTranslatorResults": true,

    "blackListDomains": [],
    "blackListTags": ["script", "head", "footer", "header", "style", "button", "iframe", "img", "picture", "video", "svg", "span", "form", "nav", "menu", "input"]
}