{
    "resultPreviewLimit": 5,
    "resultLimit": 20,

    "defaultBrowser": "google",
    "browserPrefixes": {
        "-g": "google",
        "-d": "duckduckgo",
        "-b": "brave"
    },
    "browserSearch": {
        "google": "https://www.google.com/search?q=",
        "duckduckgo": "https://html.duckduckgo.com/html?q=",
